# Uncomment the next line to define a global platform for your project
 platform :ios, '15.5'

target 'MyChatApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MyChatApp

# Firebase
pod 'FirebaseCore'
pod 'FirebaseAuth'
pod 'FirebaseDatabase'
pod 'FirebaseStorage'
pod 'FirebaseAnalytics'
pod 'FirebaseCrashlytics'

# Facebook
pod 'FBSDKLoginKit'

# Google
pod 'GoogleSignIn'

# Other
pod 'MessageKit'
pod 'SDWebImage'
pod 'JGProgressHUD'

end
